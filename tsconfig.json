{
  "compilerOptions": {
    "target": "ES2019",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "noImplicitAny": true,
    
    "skipLibCheck": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "allowJs": false,
    "checkJs": false,
    "rootDir": "src",
    "baseUrl": ".",
    "types": ["node"],
    "noEmit": true
  },
  "include": [
    "src/**/*.ts"
  ]
}
