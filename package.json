{
  "name": "@khgame/tables",
  "version": "0.1.16",
  "description": "excel => json/js/ts/ts-interface",
  "main": "./lib/index.js",
  "bin": {
    "tables": "./lib/exec.js"
  },
  "scripts": {
    "build": "rimraf ./lib/ && babel ./src --extensions '.ts,.js' --source-maps inline -d ./lib --copy-files LICENSE README.md",
    "prepublishOnly": "npm run build",
    "ex:convert": "npm run build && node ./example/ex.convert.js",
    "ex:serialize": "npm run build && node ./example/ex.serialize.js",
    "ex:minirpg": "npm run build && node ./example/game_01_minirpg/serialize.js",
    "ex:click-cookies": "npm run build && node ./example/game_02_click_cookies/serialize.js",
    "ex:darkroom": "npm run build && node ./example/game_03_a_dark_room/serialize.js && node ./node_modules/hserve/bin/hserve.js -d ./example/game_03_a_dark_room/out -p 8765 -l",
    "ex:arcane": "npm run build && node ./example/game_04_arcane_depths/serialize.js && node ./node_modules/hserve/bin/hserve.js -d ./example/game_04_arcane_depths/out -p 8769 -l",
    "test:neon": "npm run build && node ./example/game_05_neon_aberration/_rebuild_data.js && node ./example/game_05_neon_aberration/serialize.js",
    "build:site": "node ./scripts/build-site.js",
    "docs:dev": "vitepress dev docs-site",
    "docs:build": "vitepress build docs-site",
    "local-install": "rimraf /usr/local/lib/node_modules/@khgame/tables && npm i . -g",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "types": "tsc -p tsconfig.types.json",
    "test": "npm run build && jest --runInBand",
    "test:watch": "jest --watch",
    "smoke": "npm run build && node ./test/smoke.js",
    "coverage": "npm run build && jest --coverage --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khgame/tables.git"
  },
  "keywords": [
    "excel",
    "game",
    "json",
    "export"
  ],
  "author": "kinghand (kinghand@foxmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khgame/tables/issues"
  },
  "homepage": "https://github.com/khgame/tables#readme",
  "dependencies": {
    "@babel/runtime": "^7.3.4",
    "@khgame/schema": "^0.0.11",
    "assert": "^1.4.1",
    "chalk": "^2.4.2",
    "deep-equal": "^1.0.1",
    "fs-extra": "^7.0.1",
    "js-xlsx": "^0.8.22",
    "kht": "^0.0.13",
    "lodash": "^4.17.11",
    "path": "^0.12.7",
    "xlsx": "^0.18.5",
    "yargs": "^13.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-typescript": "^7.27.1",
    "@types/chalk": "^0.4.31",
    "@types/deep-equal": "^1.0.4",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.5.2",
    "@types/yargs": "^17.0.33",
    "babel-jest": "^30.1.2",
    "eslint": "^5.13.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "hserve": "^0.1.3",
    "jest": "^30.1.3",
    "ts-node": "^8.0.2",
    "tslint": "^5.13.0",
    "typescript": "^5.6.3",
    "vitepress": "^1.6.4"
  },
  "files": [
    "lib"
  ],
  "types": "lib/index.d.ts"
}
