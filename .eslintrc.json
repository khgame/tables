{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "standard",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "rules": {
    "semi": "off",
    "indent": "off",
    "object-curly-spacing": "off",
    "no-unused-vars": "off",
    "space-before-function-paren": "off",
    "quotes": "off"
  }
}
